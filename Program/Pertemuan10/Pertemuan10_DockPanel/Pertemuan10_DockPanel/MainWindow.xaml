<Window x:Class="Pertemuan10_DockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pertemuan10_DockPanel"
        mc:Ignorable="d"
        Title="DockPanel" Height="250" Width="250">
    <DockPanel>
        <Button DockPanel.Dock="Left" Width="50">Kiri</Button>
        <Button DockPanel.Dock="Top" Height="50">Atas</Button>
        <Button DockPanel.Dock="Right"  Width="50">Kanan</Button>
        <Button DockPanel.Dock="Bottom" Height="50">Bawah</Button>
        <Button>Tengah</Button>
    </DockPanel>
</Window>
