<Window x:Class="Pertemuan10_DockPanel.DockPanel2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pertemuan10_DockPanel"
        mc:Ignorable="d"
        Title="DockPanel2" Height="350" Width="604">
    <Grid>
        <DockPanel LastChildFill="True">
            <Button Content="Atas" DockPanel.Dock="Top" Click="Click_Me"/>
            <Button Content="Bawah" DockPanel.Dock="Bottom" Click="Click_Me"/>
            <Button Content="Kiri" Click="Click_Me"/>
            <Button Content="Kanan" DockPanel.Dock="Right" Click="Click_Me"/>
            <Button Content="Tengah" Click="Click_Me"/>
            
        </DockPanel>
            
    </Grid>
</Window>
