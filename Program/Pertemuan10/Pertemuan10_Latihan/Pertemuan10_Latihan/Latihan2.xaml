<Window x:Class="Pertemuan10_Latihan.Latihan2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pertemuan10_Latihan"
        mc:Ignorable="d"
        Title="StackPanel2" Height="270" Width="450">

    <StackPanel Orientation="Horizontal">
        <Button x:Name="button" Content="Button1" Margin="10,10,10,180" Width="60" Height="30"/>
        <Button x:Name="button1" Content="Button2" Margin="10" Width="60" Height="30"/>
        <Button x:Name="button2" Content="Button3" Margin="0,10,0,-150" Width="60" Height="30"/>
        <Button x:Name="button3" Content="Button4" Margin="0,10,0,-150" Width="60" Height="30"/>
        <Button x:Name="button4" Content="Button5" Margin="10" Width="60" Height="30"/>
        <Button x:Name="button5" Content="Button6" Margin="10,10,10,180" Width="60" Height="30"/>
    </StackPanel>
</Window>
